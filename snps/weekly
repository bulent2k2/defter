* test.bash
test () {
    src_path=$1
    src_dirs=$2
    tgt_path=$3
    tgt_name=$4

    cd $src_path
    pwd
    ls $src_dirs
    mkdir -p $tgt_path
    gtar -zcf $tgt_path/$tgt_name.tgz $src_dirs
    cd $tgt_path
    gtar -zxf $tgt_name.tgz
    pwd
    ls -l
}

